{
  "name": "javbus-api",
  "version": "1.6.0",
  "description": "JavBus API",
  "main": "index.js",
  "homepage": "https://github.com/ovnrain/javbus-api#readme",
  "scripts": {
    "start": "node -r dotenv/config dist/server.js",
    "dev": "tsx watch --clear-screen=false -r dotenv/config src/server.ts",
    "build": "npm run lint && tsc && prettier \"./dist/**/*.js\" --write --ignore-path=./.prettierignore",
    "format": "prettier --write 'src/**/*.ts'",
    "lint": "eslint --config .eslintrc.commit.json src/**/*.ts",
    "prepare": "husky install"
  },
  "keywords": [
    "javbus",
    "api"
  ],
  "author": {
    "name": "ovnrain",
    "email": "ovnrain@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ovnrain/javbus-api.git"
  },
  "license": "MIT",
  "devDependencies": {
    "@tsconfig/esm": "^1.0.4",
    "@tsconfig/node18": "^18.2.1",
    "@tsconfig/strictest": "^2.0.1",
    "@types/bytes": "^3.1.1",
    "@types/express": "^4.17.17",
    "@types/http-errors": "^2.0.1",
    "@types/node": "^20.5.7",
    "@types/probe-image-size": "^7.2.0",
    "@types/qs": "^6.9.7",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.2",
    "tsx": "^3.12.7",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "bytes": "^3.1.2",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "got": "^13.0.0",
    "http-errors": "^2.0.0",
    "https-proxy-agent": "^7.0.1",
    "node-html-parser": "^6.1.6",
    "probe-image-size": "^7.2.3",
    "qs": "^6.11.2",
    "socks-proxy-agent": "^8.0.1"
  },
  "type": "module"
}
